<subscribers>
	<phpclass name="consumer_service_allot_manager1">
		<method name="resetManagerConf">
			<event>task.allot.mgr.gt80</event>
		</method>
		<method name="cutManagerNeedFollowNum">
			<event>task.followed</event>
		</method>
	</phpclass>

    <phpclass name="consumer_service_allot_manager2">
        <method name="toVerifyManager">
            <event>task.need.verify</event>
        </method>
    </phpclass>

	<phpclass name="consumer_service_allot_order">
		<method name="checker">
			<event>task.created</event>
			<event>task.edited</event>
			<event>task.reseted.manager</event>
			<event>task.closed</event>
		</method>
		<method name="manager">
			<event>task.need.allot</event>
		</method>
		<method name="cutManagerTodayAllotConf">
			<event>task.closed</event>
		</method>
		<method name="resetManager">
			<event>task.follow.tag.reseted</event>
		</method>
	</phpclass>

    <phpclass name="consumer_service_allot_bschance">
        <method name="bsallot">
            <event>bschance.need.allot</event>
        </method>
        <method name="resetManager">
            <event>bschance.reset.manager</event>
        </method>
    </phpclass>

    <phpclass name="consumer_service_allot_updatemanagerinfo">
        <method name="updateManagerInfo">
            <event>task.managerallot.updatemanagerinfo</event>
        </method>
    </phpclass>
</subscribers>
