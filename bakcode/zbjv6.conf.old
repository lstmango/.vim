#task.t6.zbj.com
server {
	listen   80;
	charset utf-8;
	server_name task.t6.zbj.com;
	index index.html index.htm index.php;
	root    /data/www/html/zbjv6/trade;
#rewrite_log on;
#error_log /var/log/nginx/rule.log notice;

	access_log   /var/log/nginx/trade.log;
	location / {
		include /data/www/html/zbjv6/trade/htanginx;
		try_files $uri $uri/ =404;
	}
	include /etc/nginx/cgi.conf;
}

