	/*
	 *商机盲抢池任务堆积分配[禁止调用]
	 */
	public function allotGrabpoolBschance($initData=array()){
		$totalNum=intval($initData['totalNum']);
		if($totalNum<=0){
			return '参数错误';
		}
		$grabTaskArray=$this->getCanGrabArray();
		if(!$grabTaskArray){
			return 'no data';
		}
		$cache = new zbj_lib_cache('memcache');
		$allotTaskId=array();
		foreach($grabTaskArray as $bsValue){
			$grabData['task_id']=$bsValue['task_id'];
			$grabData['manager_id']=-1;
			$grabData['type']=2;
			$insertBack=$this->insertAllotGrab($grabData);
			if($insertBack){
				$allotTaskId[]=$bsValue['task_id'];
			}
		}
		if(!$allotTaskId){
			return 'no data2';
		}
		$cachekey2= 'bschance.zhubajie.allot.grabpool.manager_total_num';
		$cache->set($cachekey2,$totalNum,86400);
		foreach($allotTaskId as $task_id){
			$data['task_id']=$task_id;
			$data['source']=3;
			$zbjAllotBsCheck=new zbj_service_allot_bscheck();
			$result=$zbjAllotBsCheck->bsRandPoolAllot($data);
			if($result==false){
				$condition=array(
					'task_id'  => $task_id,
					'type'  => 2,
				);
				$taskAllotGrabModule = zbj_model_api::get('zbj_model_mk_taskallotgrab');
				$delResult=$taskAllotGrabModule->delete($condition);				
				if(!$delResult){
					var_dump($task_id.'清除盲抢标记失败');
				}
			}
			var_dump($task_id);
			var_dump($result);
		}
		
		return true;
	}
