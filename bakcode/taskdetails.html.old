            $("#witkey-loading").show();
            $.ajax({
                url : ajaxUrl + taskId,
                data : data,
                dataType :"json",
                type :"post",
                success : function(json){
                    if(json.state == 1){
                        if(subtype == 1 ){
                            $('#search-other').show();
