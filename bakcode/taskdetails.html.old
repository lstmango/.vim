    $("body").on('click', '.filter-search', function(){
        var subtype = $(this).attr("type");
        var ajaxUrl = ($(this).attr('data-iswebsitespecialmodes') == 1)? '/task/SellerFiltersDistributeLeaflets-id-':'/task/SellerFiltersInvitation-id-';
