    // 获取筛选条件
    function getFilterData(){
        var formData = {};
        // category3id
        if($("#serach-select").length > 0){
            formData.category3id = $("#serach-select").val();
        }else{
            formData.category1id = $("#j-category1id").val();
            formData.category2id = $("#j-category2id").val();
            formData.category3id = $("#j-category3id").val();
        }
        // attributetype
        formData.attributetype = {};
        var items = $("#search-itm-box").find('.search-item');
        var itemsValue = [];
        items.each(function(){
            var data  = {};
            var key = $(this).attr('data-extid');
            var value = [];
            var valuesEles = $(this).find('input:checked');
            valuesEles.each(function(){
                var self = $(this);
                if(self.val() == 0){
                    value.push(self.val() + '-' + self.closest('span').find('.other-input').val());
                }else if(self.val()){
                    value.push(self.val() + '-' + self.parent('label').text());
                }
            })
            formData.attributetype[key] = value.join('|');
        })

        // area
        var isArea = $("#area-box").find('input[name=isarea]').val() == 1?true:false;
        if($("#province").val()){
            if(isArea){
                formData.area = $("#province").val().join(',')
            }else{
                formData.area = 0;
            }
        }else{
            formData.area = 0;
        }
        //amount
        formData.amount = $("#now-amount").val();
        //certtype
        formData.certtype = $("#certtype").val();
        // 后端要这样的数据
        formData.otherkey = '0-' + $("#otherkey").val();
        return formData;
    }

