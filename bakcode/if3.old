#! /bin/bash
if ! test -e ./return 
then
	echo 'not exist';
elif ! test -r ./return
then
	echo 'not r';
elif ! test -w ./return
then
	echo 'not w';
elif ! test -x ./return
then
	echo 'not x';
elif test 3 -le 2
then
	echo '3>2';
#elif test -f ./return -a test -e ./return
#elif [ -f ./return ] && [ -e ./return ]
#当用[]要用&&的时候需要用两个[]才行
elif test -f ./return  && test -e ./return 
then
	echo '-f,-o';
fi
